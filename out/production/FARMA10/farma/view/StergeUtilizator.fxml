<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="550.0" stylesheets="@global.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="farma.view.StergeUtilizatorController">
   <children>
      <VBox layoutX="135.0" layoutY="50.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" styleClass="titleHBox">
               <children>
                  <Label styleClass="titleLabel" text="Stergere utilizator">
                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <Label styleClass="normalLabel" text="Selectati utilizatorul pentru dezactivarea contului">
               <VBox.margin>
                  <Insets left="20.0" top="40.0" />
               </VBox.margin>
            </Label>
            <ComboBox fx:id="selectieCB" prefHeight="37.0" prefWidth="344.0">
               <VBox.margin>
                  <Insets left="40.0" top="20.0" />
               </VBox.margin>
            </ComboBox>
            <Label fx:id="msgLB" styleClass="msgLabel">
               <VBox.margin>
                  <Insets left="20.0" top="20.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" />
               </VBox.margin>
               <children>
                  <Button maxHeight="60.0" maxWidth="120.0" minHeight="60.0" minWidth="120.0" mnemonicParsing="false" onAction="#butonOk" prefHeight="60.0" prefWidth="120.0" styleClass="success" text="OK">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button maxHeight="60.0" maxWidth="120.0" minHeight="60.0" minWidth="120.0" mnemonicParsing="false" onAction="#butonBack" prefHeight="60.0" prefWidth="120.0" styleClass="primary" text="Anulare">
                     <HBox.margin>
                        <Insets right="40.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
